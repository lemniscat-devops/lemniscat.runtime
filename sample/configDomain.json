{
    "domain.name": "CloudBooster",
    "domain.code": "cb",
    "domaine.ADgroup": "SmartPlace-All",
    "monitor.enable": true,
    "monitor.solution": "Datadog",
    "code.enable": true,
    "code.solution": "GitHub",
    "build.enable": true,
    "build.solution": "Jenkins",
    "operate.enable": true,
    "operate.solution": "Azure",
    "operate.tfBackend": "azurerm",
    "nc.projectName": "${{ appName }}-${{ serviceLevel }}",
    "nc.rgName": "rg-${{ env.env3 }}-${{scope}}-${{ domain.code }}-${{appName}}-${{serviceLevel}}",
    "nc.workspace.stName": "stws${{env.env1}}${{scopeShort}}${{domain.code}}${{CN}}${{serviceLevelShort}}",
    "nc.stName": "st${{env.env1}}${{domain.code}}${{CN}}${{serviceLevelShort}}${{instanceCode}}",
    "nc.vNetName": "vnet-${{ env.env3 }}-${{ domain.code }}-${{ env.location }}",
    "nc.vNetRg": "rg-net-${{ env.env3 }}-${{ domain.code }}",
    "nc.subnetName": "snet-${{ pepSubnetPrefix }}-${{ env.env3 }}-${{ domain.code }}-${{ serviceLevel}}",
    "regex.teamName": "^SmartPlace-(?<scopeName>.{0,7}).*$",
    "regex.developerEmail": "(?>(?<car1>\\w)[^.@]*\\.(?<car2>\\w\\w)\\w*|(?<car3>\\w)[^-@]*-(?<car4>\\w\\w)\\w*)@.*",
    "workspace.owner.default.role": "Reader",
    "product.owner.default.role": "Reader",
    "workspace.rbac": [
        {
            "principalName": "${{ ownerPrincipalName }}",
            "principalType": "${{ ownerPrincipalType }}",
            "role": "${{ workspace.owner.default.role }}",
            "scope": "/subscriptions/${{ env.subscriptionId }}/resourceGroups/${{ nc.rgName }}"
        },
        {
            "principalName": "${{ ownerPrincipalName }}",
            "principalType": "${{ ownerPrincipalType }}",
            "role": "Contributor",
            "scope": "/subscriptions/${{ env.subscriptionId }}/resourceGroups/${{ nc.rgName }}"
        }
    ],
    "storageaccount.product.rbac": [
        {
            "principalName": "${{ ownerPrincipalName }}",
            "principalType": "${{ ownerPrincipalType }}",
            "role": "${{ product.owner.default.role }}",
            "scope": "/subscriptions/${{ env.subscriptionId }}/resourceGroups/${{ nc.rgName }}/providers/Microsoft.Storage/storageAccounts/${{ nc.stName }}"
        }
    ]
}
